<?xml version="1.0" encoding="UTF-8"?>

<web-app>



	<!-- Register Servlet -->
	<servlet>
		<servlet-name>RServlet1</servlet-name>
		<servlet-class>com.techblog.servlets.RegisterServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>RServlet1</servlet-name>
		<url-pattern>/xyz</url-pattern>
	</servlet-mapping>
	
	

<!-- login servlet starts -->
	<servlet>
		<servlet-name>LServlet1</servlet-name>
		<servlet-class>com.techblog.servlets.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>LServlet1</servlet-name>
		<url-pattern>/login</url-pattern>

	</servlet-mapping>

<!-- login servlet ends -->



	
	
	<!-- logout servlet 2 -->
	<servlet>
		<servlet-name>LServlet3</servlet-name>
		<servlet-class>com.techblog.servlets.LogoutServlet2</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LServlet3</servlet-name>
		<url-pattern>/lll</url-pattern>
	</servlet-mapping>

	
	
	
	<!-- post servlet  -->
	<servlet>
		<servlet-name>post</servlet-name>
		<servlet-class>com.techblog.servlets.AddPostServlet</servlet-class>

	</servlet>
	<servlet-mapping>

		<servlet-name>post</servlet-name>
		<url-pattern>/AddPostServlet</url-pattern>

	</servlet-mapping>

<!-- post servlet ends -->





	

</web-app>  